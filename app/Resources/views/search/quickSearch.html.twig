{% extends 'base.html.twig' %}

{% block title %}
    Search Results - {{ parent() }} - {{ parent() }}
{% endblock %}

{% block breadcrumb %}
    {{ parent() }}
    <li class="breadcrumb-item active">Quick search</li>
{% endblock %}

{% block body %}
    <h1>Results for "<i>{{ search }}</i>"</h1>
    <p class="d-print-none">Too many results ? Try the <a href="{{ path('advanced-search', { 'q': search }) }}">Advanced Search</a></p>

    {% include ":search:searchResults.html.twig" with { 'search': search } %}
{% endblock %}
