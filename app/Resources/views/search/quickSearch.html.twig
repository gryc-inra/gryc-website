{% extends 'base.html.twig' %}

{%block title %}
    Search Results - {{ parent() }}
{% endblock %}

{% block breadcrumb %}
    {{ parent() }}
    <li class="active">Quick search</li>
{% endblock %}

{% block body %}
    <h1>Results for "<i>{{ search }}</i>"</h1>
    <p>Too many results ? Try the <a href="{{ path('advanced-search-with-keyword', { 'search': search }) }}">Advanced Search</a></p>

    {% include ":search:searchResults.html.twig" with { 'search': search } %}
{% endblock %}
